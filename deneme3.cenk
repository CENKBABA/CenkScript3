yazdır bölüm(sayı1,sayı2)
yazdır bölüm2(sayı1,sayı2)


fonksiyon asal(a){
	eğer(a<2){
		döndür 0
	}
	bl=2
	dnd=1
	sürece(bl<a){
		eğer(kalan(a,bl)==0){
			dnd=0
			kır
		}
		bl=bl+1
	}
	döndür dnd
}
