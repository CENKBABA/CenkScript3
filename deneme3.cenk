fonksiyon üs(a,b){
	dnd=1
	tekrarla(b){
		dnd=dnd*a
	}
	döndür dnd
}
fonksiyon kalan(a,b){
	sürece(b<=a){
		a=a-b
	}
	döndür a
}
fonksiyon bölüm(a,b){
	döndür (a-kalan(a,b))/b
}
fonksiyon bölüm2(a,b){
	dnd=0
	sürece(b<=a){
		a=a-b
		dnd=dnd+1
	}
	döndür dnd
}

